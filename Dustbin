class Dustbin{
 c
 onstructer(x, y, width, height);
   var options={
       isStatic : false
   }
      this.body - Bodies.rectangle(x,y,width,height,options);
   this.width = width;
   this.height = height;
   World.add(World, this.body);  
}

    display(){
        var position = this.body.position;
        rectMode(CENTER);
        fill ("Yellow");
        rect(position.x, position.y, this.width, this.height);
    }


World.add(World, this.body); 
}